{
  "name": "react-magma-icons",
  "version": "1.0.2",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "dist/react-magma-icons.esm.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "babel-build": "babel src --out-dir dist --extensions '.tsx','.ts'",
    "compiler-checks": "tsc --noEmit --project './tsconfig.json'",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-watch-no-covg": "jest --watch --coverage=false",
    "covg": "open coverage/lcov-report/index.html",
    "prebuild": "rimraf 'dist'",
    "build": "tsdx build",
    "build-watch": "tsdx watch",
    "lint": "npm run tsc && npm run tslint",
    "tsc": "tsc --noEmit",
    "tslint": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": "eslint src/**/*.test.js",
    "*.{ts,tsx}": [
      "tslint --project './tsconfig.json'"
    ]
  },
  "publishConfig": {
    "registry": "http://nexus.mindtap.corp.web/content/repositories/npm.cengage/"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.4",
    "@types/react": "^16.9.0",
    "eslint": "^5.16.0",
    "eslint-config-cengage": "^0.5.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-unicorn": "^8.0.2",
    "jest-axe": "3.1.0",
    "jest-extended": "^0.11.5",
    "lint-staged": "^10.2.11",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.1",
    "tslint": "^6.1.3",
    "tslint-config-cengage": "^0.1.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react-a11y": "^1.0.0",
    "typescript": "^3.9.7",
    "uuid": "^8.3.0"
  },
  "dependencies": {
    "@types/uuid": "^8.3.0"
  }
}
