{
  "plugins": [
    ["emotion", { "autoLabel": true }],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": false
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": ["dynamic-import-node"],
      "presets": [
        "@babel/preset-react",
        "@babel/preset-typescript",
        [
          "@babel/preset-env",
          {
            "useBuiltIns": "entry",
            "corejs": "3.*.*"
          }
        ]
      ]
    }
  }
}
