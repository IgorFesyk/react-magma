{
  "settings": {
    "react": {
      "version": "16.4"
    }
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "rules": {
    "no-unused-variables": true
  },
  "env": {
    "jest": true,
    "browser": true,
    "es6": true,
    "commonjs": true,
    "node": true
  },
  "globals": {
    "Cypress": true,
    "cy": true,
    "expect": true
  }
}
